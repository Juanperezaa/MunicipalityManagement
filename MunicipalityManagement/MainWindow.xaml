<Window x:Class="MunicipalityManagement.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MunicipalityManagement"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid x:Name="First" Background="White">
        <Button x:Name="Click1" Content="Select path" HorizontalAlignment="Center" VerticalAlignment="Bottom" Click="Button_Click" Height="83" Width="130" FontSize="22" FontFamily="Times New Roman" BorderBrush="#FF0C0C0C" Background="#FFD85252" Margin="0,120,0,0"/>
        <DataGrid x:Name="Table" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="0,0,0,88" BorderBrush="White" Foreground="Black" Background="White"/>
    </Grid>
</Window>
